    function loadCountNoticationTop(info){
        let session_id = info.session_id;
        let customer_sess = info.customer_id;
        let is_login = info.islogin;
	$jq.ajax({
	    url: COUNT_TOP_NOTIFICATION_URL,
	    method: 'post',
	    data: {
		session_id: session_id,
                customer_sess : customer_sess,
                is_login : is_login
	    },
	    success: function (result) {
                $unseen_number = $jq(".top-notification-button-unseen");
                if(result['success']){
                    $unseen_number.attr('data', result['number']);
                    if(result['number'] > 0){
                        $jq('.fhs_notification_unseen').text('('+result['number']+")");
                        if(result['number'] > 99){
                            result['number'] = '99+';
                        }
                        $unseen_number.text(result['number']);
                        $unseen_number.show();
                    }
                }
		
	    }
	});
    }
    
    function addTopNotification(item){
        let url = "/customer/notification";
	if(item['url']){
	    url = item['url'];
	}
	let icon_active = '';
	if(item['seen_status'] == "0"){
	    icon_active = '<i class="ico_dot_red"></i>';
	}
	let icon = 'ico_noti_other';
        if(item['page_type']){
	    switch(item['page_type']){
		case 'fpoint':
		    icon = 'ico_noti_fpoint';
		break;
		case 'coupon':
		case 'event':
		    icon = 'ico_noti_promotion';
		break;
		case 'order':
		    icon = 'ico_noti_bill';
		break;
	    }
        }
        
        let item_html =  "<a class='top-notification-item fhs_column_left fhs_mouse_point clear' onclick='setSeenNoti($jq(this), "+item['id']+");' href='"+url+"'>"
			    +"<div class='fhs_top_stretch' style='width:100%;'>"
