                <script>
                    var is_clicked_more = false;
                    var padding_bottom = 16;
                    var btnShowMoreLanguage = {
                        viewMore: 'Xem thêm<span class="icon_seemore_blue mobile_only down" style="margin-left:4px;"></span>',
                        viewLess: 'Rút gọn<span class="icon_seemore_blue mobile_only up" style="margin-left:4px;"></span>'
                    }
                    $jq(window).on('skeleton_product_view_loaded', function(){
                        initBtnShowMore();
                        $jq(window).resize(function(){
                            let h_resize = $jq('#product_tabs_description_contents').prop('scrollHeight')
                            if(h_resize > padding_bottom){
                                h_resize = h_resize - padding_bottom
                            }
                            let display = 'block';
                            if(getIsClickedMore()){
                                display = 'none';
                            }
                            let $product_tabs_description_contents = $jq('#product_tabs_description_contents');
                            if (h_resize < 300) {
                                $jq('.product_tabs_description_contents_gradient').css('display', 'none');
                                $jq('#desc_viewmore').fadeOut(0);
                            }else{
                                $jq('.product_tabs_description_contents_gradient').css('display', display);
                                $jq('#desc_viewmore').fadeIn(0);
                            }
                        });
                    });
                    $jq('#btn_showmore').click(function () {
                        let $btn_showmore = $jq(this);
                        let block_content_product_detail = $jq('#block-content-product-detail');
                        let block_info_detail_2 = $jq('#block_info_detail_2');
                        let $product_tabs_description_contents = $jq('#product_tabs_description_contents');
                        let h = $product_tabs_description_contents.height();
                        let h_product_detail = block_content_product_detail.height(); 
                        setIsClickedMore(false)
                        if (h >= 300) {
                            if(isInViewport('#block_info_detail_2')){                                
                                $product_tabs_description_contents.css('min-height', '300px').slideUp({
                                duration: 350,
                                complete : () => {
                                    $jq('.product_tabs_description_contents_gradient').css('display', 'block');
                                    $product_tabs_description_contents.css('max-height', '300px');  
                                    $btn_showmore.html(btnShowMoreLanguage.viewMore);
                                }
                            });
                            } else {
                                $jq('html, body').animate({
                                scrollTop: block_content_product_detail.offset().top
                            }, 700, function() {
                                $jq('.product_tabs_description_contents_gradient').css('display', 'block');
                                $product_tabs_description_contents.css('max-height', '300px');
                                $jq("#product_tabs_description_contents_gradient").css('min-height', '300px').slideUp(350, function() {
                                });
                                $btn_showmore.html(btnShowMoreLanguage.viewMore);
                            }); 
                            }
                        } else {
                            $jq('.product_tabs_description_contents_gradient').css('display', 'none');
                            $product_tabs_description_contents.css('max-height', 'none');
                            $product_tabs_description_contents.css('min-height', 'none');
                            $product_tabs_description_contents.css('height', 'auto').slideDown();
                            setIsClickedMore(true)
                            $btn_showmore.html(btnShowMoreLanguage.viewLess);
                        }
                    });

                    function isInViewport(element) {
                        var elementTop = $jq(element).offset().top;
                        var elementBottom = elementTop + $jq(element).outerHeight();

                        var viewportTop = $jq(window).scrollTop();
                        var viewportBottom = viewportTop + $jq(window).height();

                        return elementBottom > viewportTop && elementTop < viewportBottom;
                    }
                    function getIsClickedMore(){
                        return is_clicked_more;
                    }
                    function setIsClickedMore(isClicked){
                        is_clicked_more = isClicked
                    }

                    function initBtnShowMore(){
                        let h = $jq('#product_tabs_description_contents').prop('scrollHeight')
                        if(h > padding_bottom){
                            h = h - padding_bottom
                        }
                        let $product_tabs_description_contents = $jq('#product_tabs_description_contents');
                        let $gradient= $jq('.product_tabs_description_contents_gradient');
                        let $btn_showmore = $jq('#btn_showmore');
                        $gradient.removeAttr('style');
                        $btn_showmore.html(btnShowMoreLanguage.viewMore);
                        if (h < 300) {
                            $gradient.css('display', 'none');
                            $jq('#desc_viewmore').fadeOut(0);
                        }else{
                            $jq('#desc_viewmore').fadeIn(0);
                            $gradient.css('display', 'block');
                            $product_tabs_description_contents.css('max-height', '300px');
                        }
                    }

                   $jq(window).on('desc_content_change', function() {
                        is_clicked_more = false;
                        initBtnShowMore();
                   });
                </script>
