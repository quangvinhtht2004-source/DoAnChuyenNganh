			<link rel="stylesheet" type="text/css" href="/skin/frontend/ma_vanese/fahasa/css/flashsale.css?q=11565" media="all" />
		<script type="text/javascript" src="/js/lib/flashsale.js?q=11565"></script>
		<script type="text/javascript">
			/*
			 *  Get product data
			 */

			$jq(window).on('skeleton_product_view_loaded', function(){
				getPrice(188707);
			});
			const FLASHSALE_URL = "/node_api/flashsale/product";
			const FLASHSALES_URL = "/node_api/flashsale/products";
			var FLASHSALE_MOENGAGE = null;
			const TEXT_LABELS = {
				'ket_thuc': "Kết thúc trong",
			}
			var is_loaded_all_price = false;
			var flashsale = new FlashSale();
			function getPrice(product_id, product_ids) {
								if (!fhs_account.isEmpty(product_ids)) {
					if (is_loaded_all_price) {
						return;
					}
					$jq.ajax({
						url: FLASHSALES_URL,
						method: 'post',
						data: {
							product_ids: product_ids
						},
						error: function(XMLHttpRequest, textStatus, errorThrown) {
							flashsale.onShowPriceProductFlashsale()
							$jq("#catalog-product-details-discount").show();
						},
						success: function(data) {
														is_loaded_all_price = true;

							if (!data.result) {
																flashsale.onShowPriceProductFlashsale()
								$jq("#catalog-product-details-discount").show();
								return;
							}
							if (data['products']) {
								if (data['products'][product_id]) {
									// var flashsale = new FlashSale();
									flashsale.initProductPage(data['products'][product_id], data.period, IS_MOBILE, TEXT_LABELS);
								}
								else{
									flashsale.onHideFlashsaleProductPage()
								}
							}
						}
					});
				} else {
					$jq.ajax({
						url: FLASHSALE_URL,
						method: 'post',
						data: {
							product_id: PRODUCT_ID
						},
						error: function(XMLHttpRequest, textStatus, errorThrown) {
							flashsale.onShowPriceProductFlashsale()
							$jq("#catalog-product-details-discount").show();
							if(typeof moengageProductView === 'function')moengageProductView();
						},
						success: function(data) {
							
							// tracking product_view moengage : 
							var productFlashsale = null;
							if(data?.result && data?.product && Object.keys(data.product).length > 0){
								productFlashsale = {};
								const productReponse = data.product;
								const flashsale_total_sold = productReponse?.total_sold ?? 0;
								const flashsale_total_items = productReponse?.total_items ?? 0;
								if (flashsale_total_sold < flashsale_total_items) {
									const flashsale_final_price = productReponse?.flashsale_price ?? 0;
        							const flashsale_original_price = productReponse?.original_price ?? 0;
        							const flashsale_discount_percent = productReponse?.discount ?? 0;
									productFlashsale['final_price'] = parseInt(flashsale_final_price).toString();
	        						productFlashsale['price'] = parseInt(flashsale_original_price).toString();
    	    						productFlashsale['discount'] = flashsale_discount_percent.toString();
        						}
							}

							if(typeof moengageProductView === 'function')moengageProductView(productFlashsale);

							if (!data.result) {
																flashsale.onShowPriceProductFlashsale();
								$jq("#catalog-product-details-discount").show();
								return;
							}

							// var flashsale = new FlashSale();
							flashsale.initProductPage(data.product, data.period, IS_MOBILE, TEXT_LABELS);
							setTimeout(() => {flashsale.onShowPriceProductFlashsale();}, 100);
						}
					});
				}
			}
		</script>
